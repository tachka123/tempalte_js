{
	"name": "node-boilerplate",
	"version": "1.0.0",
	"description": "Boilerplate for nodejs application with webpack and configured eslint",
	"private": true,
	"main": "src/main.js",
	"engines": {
		"node": ">=10.16.0"
	},
	"scripts": {
		"build:dev": "webpack --mode=development",
		"build:prod": "webpack --mode=production",
		"start:dev": "node --inspect-brk=9898 ./dist/main.js",
		"prestart:dev": "npm run build:dev",
		"lint": "eslint .",
		"lint-fix": "eslint --fix ."
	},
	"keywords": [
		"node",
		"webpack",
		"eslint",
		".eslintrc",
		"javascript",
		"boilerplate",
		"template"
	],
	"author": "Sheva",
	"license": "MIT",
	"devDependencies": {
		"babel-eslint": "^10.0.1",
		"eslint": "^5.16.0",
		"eslint-config-canonical": "^17.1.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-import-resolver-node": "^0.3.2",
		"eslint-import-resolver-webpack": "^0.11.1",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-filenames": "^1.3.2",
		"eslint-plugin-flowtype": "^3.10.1",
		"eslint-plugin-fp": "^2.3.0",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-jsdoc": "^7.2.3",
		"eslint-plugin-no-use-extend-native": "^0.4.0",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-promise": "^4.1.1",
		"eslint-plugin-sort-keys-fix": "^1.0.1",
		"eslint-plugin-unicorn": "^9.1.0",
		"prettier": "^1.18.2",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^3.7.1",
		"webpack-node-externals": "^1.7.2"
	},
	"dependencies": {}
}
